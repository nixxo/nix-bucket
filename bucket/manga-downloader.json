    "homepage": "https://jdownloader.org",
    "license": {
        "identifier": "Proprietary",
        "url": "http://redsquirrel87.altervista.org/doku.php/license"
    },
    "suggest": {
        "JRE": "java/adopt8-hotspot-jre"
    },
    "url": [
        "https://bitbucket.org/Red_Squirrel/manga-downloader/downloads/latest.jar#/MangaDownloader.jar",
